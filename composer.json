{
    "name": "zoopcommerce/pixie",
    "type": "composer-installer",
    "description": "custom composer installers for dojo, havok, mystique and others",
    "license": "MIT",
    "keywords": [
        "zoop",
        "composer",
        "installer",
        "dojo",
        "havok",
        "mystique"
    ],
    "homepage": "http://github.com/zoopcommerce/pixie",
    "autoload": {
        "psr-0": {
            "Zoop\\Pixie\\": "src"
        }
    },
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "1.4.*"
    },
    "extra": {
        "class": [
            "Zoop\\Pixie\\JsInstaller",
            "Zoop\\Pixie\\MystiqueInstaller",
            "Zoop\\Pixie\\MystiqueJsInstaller"
        ]
    }
}